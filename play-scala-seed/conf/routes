# Routes
# This file defines all application routes (Higher priority routes first)
# https://www.playframework.com/documentation/latest/ScalaRouting
# ~~~~

# An example controller showing a sample home page
# GET     /first                      controllers.SpotifyController.get100first
# GET     /viral                      controllers.SpotifyController.getRecentTopViral50ByCountry
# GET     /streams                    controllers.SpotifyController.getMostStreamedPerCountry
GET     /countries/all                controllers.CountriesController.getAll
GET     /genres                     controllers.GenresController.getGenres
GET     /clean/histories            controllers.SpotifyCleanController.histories(ids: Seq[(String,String)])
GET     /clean/history              controllers.SpotifyCleanController.history(id: String)
GET     /clean/date                 controllers.SpotifyCleanController.date(d: String)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
